<odoo>

	<record id="custom_report_paper_delivery_order" model="report.paperformat">
		<field name="name">DELIVERY ORDER PDF FORMAT</field>
		<field name="default" eval="True" />
		<field name="format">A4</field>
		<field name="page_height">0</field>
		<field name="page_width">0</field>
		<field name="orientation">Portrait</field>
		<field name="margin_top">40</field>
		<field name="margin_bottom">23</field>
		<field name="margin_left">7</field>
		<field name="margin_right">7</field>
		<field name="header_line" eval="False" />
		<field name="header_spacing">5</field>
		<field name="dpi">90</field>
		<!-- <field name="report_ids" eval="[(4, ref('ab_travel_umroh.report_delivery_order_action')), (4, ref('ab_travel_umroh.report_invoice_action'))]"></field> -->
		<!-- <field name="report_ids" eval="[(4, ref('ab_travel_umroh.report_delivery_order_action'))]"></field> -->
	</record>

	<record id="custom_report_paper_customer_invoice" model="report.paperformat">
		<field name="name">CUSTOMER INVOICE PDF FORMAT</field>
		<field name="default" eval="True" />
		<field name="format">A4</field>
		<field name="page_height">0</field>
		<field name="page_width">0</field>
		<field name="orientation">Portrait</field>
		<field name="margin_top">40</field>
		<field name="margin_bottom">23</field>
		<field name="margin_left">7</field>
		<field name="margin_right">7</field>
		<field name="header_line" eval="False" />
		<field name="header_spacing">5</field>
		<field name="dpi">90</field>
		<!-- <field name="report_ids" eval="[(4, ref('ab_travel_umroh.report_delivery_order_action')), (4, ref('ab_travel_umroh.report_invoice_action'))]"></field> -->
		<!-- <field name="report_ids" eval="[(4, ref('ab_travel_umroh.report_action.report_invoice_action'))]"></field> -->
	</record>

</odoo>
